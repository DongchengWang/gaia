// what to do?
func foo(x: int, y: int, z: int): int {
    var i: int;
    var j: int;
    i=1;
    j=3;
    while (true) {
        loop {
            i=i+1;
        } while i<j;
        if i>=j {
            break;
        }
    }
    if i>=j {
        i=2;
    } else {
        i=3;
    }
    return i;
}
